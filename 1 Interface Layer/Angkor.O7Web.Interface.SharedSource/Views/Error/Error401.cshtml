@model Angkor.O7Web.Interface.SharedSource.ViewModels.ErrorViewModel
<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Angkor | 401 Error</title>
    <link href="/SharedSource/css/bootstrap.min.css" rel="stylesheet">
    <link href="/SharedSource/font-awesome/css/font-awesome.css" rel="stylesheet">
    <link href="/SharedSource/css/animate.css" rel="stylesheet">
    <link href="/SharedSource/css/style.css" rel="stylesheet">
</head>

<body class="gray-bg">
    <div class="middle-box text-center animated fadeInDown">
        <h1 style="margin-left: -80px; font-size:170px;">O7Web</h1>
        <h3 class="font-bold">Error 401 - Sin autorización</h3>        
        <div class="error-desc">
            <div class="ibox">
                <div class="ibox-title">
                    <h5>Soporte O7Solution</h5>
                </div>
                <div class="ibox-content">
                    <form class="form-horizontal">
                        <div class="form-group">
                            <label class="col-lg-2 control-label">Responsable</label>
                            <div class="col-lg-10"><p class="form-control-static">@Model.Name</p></div>
                        </div>
                        <div class="form-group">
                            <label class="col-lg-2 control-label">Teléfono</label>
                            <div class="col-lg-10"><p class="form-control-static">@Model.Phone</p></div>
                        </div>
                        <div class="form-group">
                            <label class="col-lg-2 control-label">Correo</label>
                            <div class="col-lg-10"><p class="form-control-static">@Model.Email</p></div>
                        </div>
                    </form>
                </div>
            </div>
            <div>
                <a class="btn btn-primary">
                    <i class="fa fa-home"></i>
                    Regresar al Inicio
                </a>
                <a class="btn btn-primary" href="mailto: @Model.Email?Subject=Soporte">
                    <i class="fa fa-envelope"></i>
                    Enviar correo a soporte
                </a>
            </div>
        </div>
        <div class="pull-right">
            <div class="pull-right" style="margin-top: 120px;">
                <strong>Copyright ©</strong>Desarrollado por AngkorGroup S.A.C &copy; 2016
            </div>
        </div>
    </div>

    <!-- Mainly scripts -->
    <script src="/SharedSource/js/jquery-2.1.1.js"></script>
    <script src="/SharedSource/js/bootstrap.min.js"></script>
</body>
</html>