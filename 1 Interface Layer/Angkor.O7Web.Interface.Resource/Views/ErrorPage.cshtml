@model Angkor.O7Framework.Web.Base.O7ViewModel
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <title>O7ERP | @Model.ErrorCode Error</title>

    <link href="/css/bootstrap.min.css" rel="stylesheet">
    <link href="/font-awesome/css/font-awesome.css" rel="stylesheet">
    <link href="/css/animate.css" rel="stylesheet">
    <link href="/css/style.css" rel="stylesheet">
</head>
<body class="gray-bg">
    <div class="middle-box text-center animated fadeInDown">
        <h1>@Model.ErrorCode</h1>
        <h3 class="font-bold">Error en el sistema</h3>

        <div class="error-desc">
            Disculpa las molestias, por favor informar al area de soporte sobre dicho error.
            <div class="well">
                <h3> Mensaje de error: </h3>
                @Model.ErrorMessage
            </div>
        </div>
    </div>

    <script src="/js/jquery-2.1.1.js"></script>
    <script src="/js/bootstrap.min.js"></script>
</body>
</html>
